{
  "explorer.confirmDelete": false,
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "editor.formatOnSave": true,
  "editor.minimap.enabled": false,
  "typescript.updateImportsOnFileMove.enabled": "always",
  "workbench.sideBar.location": "right",
  "workbench.editorAssociations": {
    "*.svg": "default"
  },
  "editor.tabSize": 2,
  "editor.wordWrap": "bounded",
  "editor.rulers": [80],
  "editor.scrollbar.verticalScrollbarSize": 0,
  "files.associations": {
    "*.css": "tailwindcss"
  },
  "git.confirmSync": false,
  "explorer.confirmPasteNative": false,
  "chat.disableAIFeatures": true,
  "terminal.integrated.fontFamily": "JetBrainsMono Nerd Font, monospace",
  "terminal.integrated.fontSize": 13,
  "workbench.colorTheme": "Dracula Theme",
  "workbench.colorCustomizations": {
    "editor.background": "#1a1a26",
    "editor.foreground": "#ffffff",
    "editor.selectionBackground": "#b4c4f48e",
    "editor.lineHighlightBorder": "#1a1a26",
    "scrollbar.shadow": "#0000",

    "titleBar.activeBackground": "#1a1a26",
    "titleBar.inactiveBackground": "#1a1a26",
    "titleBar.inactiveForeground": "#ffffff",
    "titleBar.activeForeground": "#ffffff",

    "tab.inactiveBackground": "#1a1a26",
    "editorGroupHeader.tabsBackground": "#1a1a26",
    "sideBar.background": "#1a1a26",
    "activityBar.background": "#262934",

    "panel.background": "#1a1a26",
    "terminal.background": "#1a1a26",

    "terminal.ansiBlack": "#32344a",
    "terminal.ansiRed": "#f7768e",
    "terminal.ansiGreen": "#9ece6a",
    "terminal.ansiYellow": "#e0af68",
    "terminal.ansiBlue": "#7aa2f7",
    "terminal.ansiMagenta": "#ad8ee6",
    "terminal.ansiCyan": "#449dab",
    "terminal.ansiWhite": "#787c99",

    "terminal.ansiBrightBlack": "#444b6a",
    "terminal.ansiBrightRed": "#ff7a93",
    "terminal.ansiBrightGreen": "#b9f27c",
    "terminal.ansiBrightYellow": "#ff9e64",
    "terminal.ansiBrightBlue": "#7da6ff",
    "terminal.ansiBrightMagenta": "#bb9af7",
    "terminal.ansiBrightCyan": "#0db9d7",
    "terminal.ansiBrightWhite": "#acb0d0"
  }
}
